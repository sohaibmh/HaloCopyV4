{"components/products/description-tabs":"<ul class=\"tabs\" data-tab>\n    {{#if product.description}}\n        <li class=\"tab is-active\">\n            <a class=\"tab-title\" href=\"#tab-description\">{{lang 'products.description'}}</a>\n        </li>\n    {{/if}}\n    {{#if product.warranty}}\n        <li class=\"tab\">\n            <a class=\"tab-title\" href=\"#tab-warranty\">{{lang 'products.warranty'}}</a>\n        </li>\n    {{/if}}\n    {{#if theme_settings.show_shipping_tab}}\n        <li class=\"tab\">\n            <a class=\"tab-title\" href=\"#tab-shipping\">{{theme_settings.show_shipping_tab}}</a>\n        </li>\n    {{/if}}\n    {{#if theme_settings.show_custom_tab}}\n        <li class=\"tab\">\n            <a class=\"tab-title\" href=\"#tab-custom\">{{theme_settings.show_custom_tab}}</a>\n        </li>\n    {{/if}}\n</ul>\n<div class=\"tabs-contents m-0\">\n    {{#if product.description}}\n        <div class=\"tab-content b-r-24\" id=\"tab-description\">\n            <div class=\"toggle-title\">\n                <a class=\"toggleLink is-open\" data-collapsible href=\"#tab-description-mobile\">\n                    <span class=\"text\">{{lang 'products.description'}}</span>\n                    <span class=\"icon-plus\">&nbsp;</span>\n                </a>\n            </div>\n            <div class=\"toggle-content is-open\" id=\"tab-description-mobile\">\n                {{{product.description}}}\n            </div>\n        </div>\n    {{/if}}\n    {{#if product.warranty}}\n        <div class=\"tab-content b-r-24\" id=\"tab-warranty\">\n            <div class=\"toggle-title\">\n                <a class=\"toggleLink\" data-collapsible href=\"#tab-warranty-mobile\">\n                    <span class=\"text\">{{lang 'products.warranty'}}</span>\n                    <span class=\"icon-plus\">&nbsp;</span>\n                </a>\n            </div>\n            <div class=\"toggle-content\" id=\"tab-warranty-mobile\">\n                <div class=\"warranty-text\">{{{product.warranty}}}</div>\n            </div>\n        </div>\n    {{/if}}\n    {{#if theme_settings.show_shipping_tab}}\n        <div class=\"tab-content b-r-24\" id=\"tab-shipping\">\n            <div class=\"toggle-title\">\n                <a class=\"toggleLink\" data-collapsible href=\"#tab-shipping-mobile\">\n                    <span class=\"text\">{{theme_settings.show_shipping_tab}}</span>\n                    <span class=\"icon-plus\">&nbsp;</span>\n                </a>\n            </div>\n            <div class=\"toggle-content\" id=\"tab-shipping-mobile\">\n                {{{region name=\"product_tab_shipping_content\"}}}\n                {{{region name=\"product_tab_shipping_content--global\"}}}\n            </div>\n        </div>\n    {{/if}}\n    {{#if theme_settings.show_custom_tab}}\n        <div class=\"tab-content b-r-24\" id=\"tab-custom\">\n            <div class=\"toggle-title\">\n                <a class=\"toggleLink\" data-collapsible href=\"#tab-custom-mobile\">\n                    <span class=\"text\">{{theme_settings.show_custom_tab}}</span>\n                    <span class=\"icon-plus\">&nbsp;</span>\n                </a>\n            </div>\n            <div class=\"toggle-content\" id=\"tab-custom-mobile\">\n                {{{region name=\"product_tab_custom_content\"}}}\n                {{{region name=\"product_tab_custom_content--global\"}}}\n            </div>\n        </div>\n    {{/if}}\n</div>\n"}
