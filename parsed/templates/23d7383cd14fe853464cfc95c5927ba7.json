{"components/common/navigation-mobile":"<nav class=\"navPages\">\n    <ul class=\"navPages-list navPages-list-mobile\"></ul>\n    <ul class=\"navPages-list navPages-list--user\" data-level-list>\n        <li class=\"navPages-item navPages-item-user\">\n            <a class=\"navPages-action navPages-action-wishlist navPages-action-end\" href=\"/wishlist.php\">\n                <span class=\"text\">\n                    <svg class=\"icon\" role=\"img\" aria-label=\"{{lang 'account.wishlists.heading'}}\">\n                        <title>{{lang 'account.wishlists.heading'}}</title>\n                        <desc>{{lang 'account.wishlists.heading'}}</desc>\n                        <use href=\"#icon-heart\"></use>\n                    </svg>\n                    {{lang 'account.wishlists.heading'}}\n                </span>\n            </a>\n        </li>\n        {{#if settings.phone_number}}\n            <li class=\"navPages-item navPages-item-user\">\n                <a class=\"navPages-action navPages-action-phone navPages-action-end\" role=\"button\" tabindex=\"-1\" href=\"tel:{{settings.phone_number}}\">\n                    <span class=\"text\">\n                        <svg class=\"icon\" role=\"img\" aria-label=\"{{lang 'account.addresses.phone'}}\">\n                            <title>{{lang 'account.addresses.phone'}}</title>\n                            <desc>{{lang 'account.addresses.phone'}}</desc>\n                            <use href=\"#icon-phone\"></use>\n                        </svg>\n                        {{settings.phone_number}}\n                    </span>\n                </a>\n            </li>\n        {{/if}}\n        {{#if theme_settings.halo_header_liveHelp}}\n            <li class=\"navPages-item navPages-item-user\">\n                <a class=\"navPages-action navPages-action-phone navPages-action-end\" href=\"{{theme_settings.halo_header_liveHelpLink}}\" aria-label=\"{{lang 'common.help'}}\">\n                    <span class=\"text\">\n                        <svg class=\"icon\" role=\"img\" aria-label=\"{{lang 'common.help'}}\">\n                            <title>{{lang 'common.help'}}</title>\n                            <desc>{{lang 'common.help'}}</desc>\n                            <use href=\"#icon-helper\"></use>\n                        </svg>\n                        {{lang 'common.help'}}\n                    </span>\n                </a>\n            </li>\n        {{/if}}\n        {{#if currency_selector.currencies.length '>' 1}}\n            <li class=\"navPages-item navPages-item-user has-dropdown currencies\" data-level=\"2\">\n                <p id=\"currency_selector\" class=\"navPages-action navPages-action-currency navPages-action-depth-max has-subMenu is-root\">\n                    <a class=\"text center\" href=\"#\">\n                        <span class=\"cur-flag d-inline-block v-a-middle\">{{{currency_selector.active_currency_flag}}}</span>\n                        <span class=\"d-inline-block v-a-middle f-root\">{{currency_selector.active_currency_code}}</span>\n                    </a>\n                    <span class=\"navPages-action-moreIcon\" aria-hidden=\"true\">\n                        <svg class=\"icon\"><use href=\"#icon-chevron-down\" /></svg>\n                    </span>\n                </p>\n                <div class=\"navPage-subMenu navPage-subMenu-horizontal halo-fadeIn\" id=\"navPages-currency\">\n                    <ul class=\"navPage-subMenu-list\">\n                        <li class=\"navPage-subMenu-item-child navPage-subMenu-title\" data-level=\"2\">\n                            <p class=\"navPage-subMenu-action navPages-action\">\n                                <span class=\"navPages-action-moreIcon\" aria-hidden=\"true\">\n                                    <svg class=\"icon\"><use href=\"#icon-arrow-prev\" /></svg>\n                                </span>\n                                <span class=\"text\">{{lang 'common.select_currency'}}