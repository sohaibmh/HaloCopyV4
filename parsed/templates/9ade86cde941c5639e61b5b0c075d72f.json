tent\" id=\"category_{{id}}\" role=\"tabpanel\" aria-hidden=\"true\" data-tab-category-id=\"{{id}}\" data-tab-category-url=\"{{url}}\">\n                                    <div class=\"loadingOverlay\"></div>\n                                    <section class=\"productCarousel haloSwiper pos-relative o-h\"\n                                        data-list-name=\"Product By Tabs\"\n                                        data-per-view-mob=\"{{../../theme_settings.cardProd_colMob}}\" \n                                        data-per-view-tab=\"{{../../theme_settings.cardProd_colTab}}\" \n                                        data-per-view-des=\"{{../../theme_settings.cardProd_col}}\" \n                                        data-swiper-pag-mob=\"true\" \n                                        data-swiper-space-tab=\"20\" \n                                        data-swiper-space-des=\"30\"\n                                        data-rows-des=\"2\"\n                                    >\n                                        <div class=\"swiper-wrapper disable-srollbar\">\n                                            {{> components/halothemes/product/halo-product-sample}}\n                                            {{> components/halothemes/product/halo-product-sample}}\n                                            {{> components/halothemes/product/halo-product-sample}}\n                                            {{> components/halothemes/product/halo-product-sample}}\n                                            {{> components/halothemes/product/halo-product-sample}}\n                                            {{> components/halothemes/product/halo-product-sample}}\n                                            {{> components/halothemes/product/halo-product-sample}}\n                                            {{> components/halothemes/product/halo-product-sample}}\n                                        </div>\n                                        <div