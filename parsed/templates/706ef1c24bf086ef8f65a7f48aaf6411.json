{"components/common/quick-search":"<form class=\"form\" onsubmit=\"return false\" data-url=\"{{urls.search}}\" data-quick-search-form>\n    <fieldset class=\"form-fieldset\">\n        <div class=\"form-field pos-relative\">\n            <label class=\"is-srOnly\" for=\"{{name}}\">{{lang \"search.quick_search.input_label\"}}</label>\n            <input class=\"form-input\" \n                   data-search-quick \n                   data-search=\"quickSearch\" \n                   name=\"{{name}}\" \n                   id=\"{{name}}\" \n                   data-error-message=\"{{lang 'search.error.empty_field'}}\" \n                   placeholder=\"{{lang 'search.quick_search.input_placeholder'}}\" \n                   autocomplete=\"off\">\n            <button type=\"submit\" class=\"button button--primary button--iconCustom pos-absolute\" aria-label=\"{{lang \"search.quick_search.input_label\"}}\"><svg class=\"icon\"><use xlink:href=\"#icon-search-2\"></use></svg></button>\n        </div>\n    </fieldset>\n</form>\n"}
