{"components/common/subscription-form":"<form class=\"form form-newsletter m-0 halo-fadeInUp\" action=\"{{urls.subscribe.action}}\" method=\"post\" data-halo-animate=\"2\">\n    <fieldset class=\"form-fieldset\">\n        <input type=\"hidden\" name=\"action\" value=\"subscribe\">\n        <input type=\"hidden\" name=\"nl_first_name\" value=\"bc\">\n        <input type=\"hidden\" name=\"check\" value=\"1\">\n        <div class=\"form-field m-0\">\n            <label class=\"form-label is-srOnly\" for=\"nl_email\">{{lang 'common.email_address'}}</label>\n            <div class=\"wrap pos-relative\">\n                <input class=\"form-input input-newsletter b-r-40\"\n                       id=\"nl_email\"\n                       name=\"nl_email\"\n                       type=\"email\"\n                       value=\"{{customer.email}}\"\n                       placeholder=\"{{lang 'newsletter.email_placeholder'}}\"\n                       aria-describedby=\"alertBox-message-text\"\n                       aria-required=\"true\"\n                       autocomplete=\"email\"\n                       required\n                >\n                <input class=\"button button--secondary button-newsletter f-bold b-none b-r-40 pos-absolute\"\n                       type=\"submit\"\n                       value=\"{{lang 'newsletter.subscribe_submit'}}\"\n                >\n            </div>\n            {{#if settings.show_newsletter_summary }}\n                <div class=\"footer-newsletter-summary p-t\">{{settings.newsletter_summary}}</div>\n            {{/if}}\n        </div>\n    </fieldset>\n</form>\n"}
