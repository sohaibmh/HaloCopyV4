{"components/common/paginator":"<nav class=\"pagination\" aria-label=\"pagination\">\n    <ul class=\"pagination-list\">\n        {{#or next previous}}\n            <li class=\"pagination-item pagination-item--previous d-none-mb{{#unless previous}} button--disable{{/unless}}\">\n                <a class=\"pagination-link d-flex a-i-center j-c-center\"\n                   href=\"{{previous}}\"\n                   {{#unless reload}}data-faceted-search-facet{{/unless}}\n                   aria-label=\"{{lang 'common.previous'}}\"\n                >\n                    <i class=\"icon\" aria-hidden=\"true\">\n                        <svg>\n                            <use href=\"#icon-arrow-prev\"></use>\n                        </svg>\n                    </i>\n                </a>\n            </li>\n        {{/or}}\n        {{#each links}}\n            <li class=\"pagination-item {{#if this.number '==' ../current}}pagination-item--current{{/if}}\">\n                <a class=\"pagination-link d-flex a-i-center j-c-center\"\n                   href=\"{{this.url}}\"\n                   {{#unless reload}}data-faceted-search-facet{{/unless}}\n                   {{#if this.number '==' ../current}}\n                   aria-current=\"page\"\n                   data-pagination-current-page-link\n                   {{/if}}\n                   aria-label=\"{{lang 'common.paginator.page_of' current=this.number total=../links.length}}\"\n                >\n                    {{this.number}}\n                </a>\n            </li>\n        {{/each}}\n\n        {{#or next previous}}\n            <li class=\"pagination-item pagination-item--next d-none-mb{{#unless next}} button--disable{{/unless}}\">\n                <a class=\"pagination-link d-flex a-i-center j-c-center\"\n                   href=\"{{next}}\"\n                   {{#unless reload}}data-faceted-search-facet{{/unless}}\n                   aria-label=\"{{lang 'common.next'}}\"\n                >\n                    <i class=\"icon\" aria-hidden=\"true\">\n                        <svg>\n                            <use href=\"#icon-arrow-next\"></use>\n                        </svg>\n                    </i>\n                </a>\n            </li>\n        {{/or}}\n    </ul>\n</nav>\n"}
