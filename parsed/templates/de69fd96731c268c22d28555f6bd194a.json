{"components/common/header":"<a href=\"#main-content\" class=\"skip-to-main-link\">{{lang 'header.skip_to_main'}}</a>\n{{#if banners.top}}\n    <div class=\"banners\" data-banner-location=\"top\">\n        {{#each (limit banners.top_metadata 1)}}\n            <div class=\"banner\" data-event-type=\"promotion\" data-entity-id=\"{{this.id}}\" data-name=\"{{this.banner-name}}\" data-position=\"{{this.location}}\" data-banner-id=\"{{this.banner-name}}\">\n                <div data-event-type=\"promotion-click\">\n                    {{{this.content}}}\n                </div>\n            </div>\n        {{/each}}\n    </div>\n{{/if}}\n<header class=\"header header-logo-position--{{theme_settings.logo-position}}\" role=\"banner\" data-halo-load=\"animation\">\n    <div class=\"halo-topHeader halo-fadeInDown\" data-prevent-quick-search-close>\n        <div class=\"container container-1600\">\n            {{#if theme_settings.logo-position '==' 'left'}}\n                <div class=\"topHeader-item z-index-1 f-1 left\">\n                    <div class=\"topHeader-item__item item--sticky-logo\">\n                        {{#if page_type '===' 'default'}}\n                            <h1 class=\"header-logo header-logo--{{theme_settings.logo-position}} d-inline-block v-a-middle\">\n                                {{> components/common/store-logo}}\n                            </h1>\n                        {{else}}\n                            <div class=\"header-logo header-logo--{{theme_settings.logo-position}} d-inline-block v-a-middle\">\n                                {{> components/common/store-logo}}\n                            </div>\n                        {{/if}}\n                        <div class=\"contact-phone d-inline-block v-a-middle\">{{{theme_settings.halo_header_customText}}}</div>\n                    </div>\n                </div>\n            {{else}}\n                <div class=\"topHeader-item z-index-2 f-1 left\">\n                    <div class=\"item--quicksearch item--searchMobile w-100 halo-search halo-search-main d-inline-block v-a-middle\">\n                        <div id=\"quickSearch\" class=\"quickSearch\">\n                            {{> components/common/quick-search name='quick_search_header'}}\n                        </div>\n                    </div>\n                    <div class=\"contact-phone d-inline-block v-a-middle\">{{{theme_settings.halo_header_customText}}}</div>\n                </div>\n            {{/if}}\n\n            {{#if theme_settings.logo-position '==' 'left'}}\n                <div class=\"topHeader-item z-index-2 f-1 center\">\n                    <div class=\"item--quicksearch item--searchMobile w-100 halo-search halo-search-main\">\n                        <div id=\"quickSearch\" class=\"quickSearch\">\n                            {{> components/common/quick-search name='quick_search_header'}}\n                        </div>\n                    </div>\n                </div>\n            {{else}}\n                <div class=\"topHeader-item z-index-1 f-1 center\">\n                    <div class=\"topHeader-item__item item--sticky-logo\">\n                        {{#if page_type '===' 'default'}}\n                            <h1 class=\"header-logo header-logo--{{theme_settings.logo-position}}\">\n                                {{> components/common/store-logo}}\n                            </h1>\n                        {{else}}\n                            <div class=\"header-logo header-logo--{{theme_settings.logo-position}}\">\n                                {{> components/common/store-logo}}\n                            </div>\n                        {{/if}}\n                    </div>\n                </div>\n            {{/if}}\n\n            <div class=\"topHeader-item z-index-1 f-1 right\">\n                {{> components/common/navigation}}\n            </div>\n        </div>\n    </div>\n\n    <div class=\"halo-middleHeader halo-fadeInDown\">\n        <div class=\"container container-1600\">\n            <div class=\"middleHeader-item left\">\n                <div class=\"middleHeader-item__item item--hamburger\">\n                    <a href=\"#\" class=\"mobileMenu-toggle\" data-mobile-menu-toggle=\"menu\">\n                        <span class=\"mobileMenu-toggleIcon\">Toggle menu</span>\n                    </a>\n                </div>\n            </div>\n            <div class=\"middleHeader-item center\">\n                <div class=\"middleHeader-item__item item--logo\">\n                    {{#if page_type '===' 'default'}}\n                        <h1 class=\"header-logo header-logo--{{theme_settings.logo-position}}\">\n                            {{> components/common/store-logo}}\n                        </h1>\n                    {{else}}\n                        <div class=\"header-logo header-logo--{{theme_settings.logo-position}}\">\n                            {{> components/common/store-logo}}\n                        </div>\n                    {{/if}}\n                </div>\n            </div>\n            <div class=\"middleHeader-item right\">\n                <div class=\"middleHeader-item__item item--searchMobile\">\n                    <button class=\"navUser-action navUser-action--quickSearch\"\n                       type=\"button\"\n                       id=\"quick-search-expand\"\n                       data-search=\"quickSearch\" \n                       aria-controls=\"quickSearch\" \n                       aria-label=\"{{lang 'common.search'}}\"\n                    >\n                        <div class=\"navUser-icon\">\n                            <svg class=\"icon\" role=\"img\" aria-label=\"{{lang 'search.quick_search.input_label'}}\">\n                                <title>{{lang 'search.quick_search.input_label'}}</title>\n                                <desc>{{lang 'search.quick_search.input_placeholder'}}</desc>\n                                <use xlink:href=\"#icon-search\"></use>\n                            </svg>\n                        </div>\n                    </button>\n                </div>\n                <div class=\"middleHeader-item__item item--account\">\n                    <a class=\"navUser-action\" {{#if customer}}href=\"{{urls.account.index}}\"{{else}}href=\"{{urls.auth.login}}\"{{/if}} aria-label=\"{{lang 'common.login'}}\" data-login-form>\n                        <div class=\"navUser-icon\">\n                            <svg class=\"icon\" role=\"img\" aria-label=\"{{lang 'common.account'}}\">\n                                <title>{{lang 'common.account'}}</title>\n                                <desc>{{lang 'common.account'}}</desc>\n                                <use xlink:href=\"#icon-account\"></use>\n                            </svg>\n                        </div>\n                    </a>\n                </div>\n                <div class=\"middleHeader-item__item item--cart halo-cart\">\n                    <a class=\"navUser-action\" data-cart-preview data-options=\"align:right\" href=\"{{urls.cart}}\" aria-label=\"{{lang 'cart.nav_aria_label'}}\">\n                        <div class=\"navUser-icon navUser-item-cartIcon\">\n                            <svg class=\"icon\" role=\"img\" aria-label=\"{{lang 'cart.preview.cart'}}\">\n                                <title>{{lang 'cart.preview.cart'}}</title>\n                                <desc>{{lang 'cart.preview.cart'}}</desc>\n                                <use xlink:href=\"#icon-cart\"></use>\n                            </svg>\n                            <span class=\"countPill d-inline-block v-a-middle center cart-quantity\">{{#if cart.items}}{{cart.quantity}}{{else}}0{{/if}}</span>\n                        </div>\n                    </a>\n                </div>\n            </div>\n        </div>\n    </div>\n\n    <div class=\"halo-bottomHeader halo-fadeInDown\">\n        <div class=\"container container-1600\">\n            <div class=\"navPages-container item--sticky-logo\" id=\"menu\" data-menu>\n                {{> components/common/navigation-menu}}\n            </div>\n            <div class=\"bottomHeader-item right\">\n                <nav class=\"navUser\">\n                    <ul class=\"navUser-section navUser-section--alt\">\n                        {{#if theme_settings.halo_header_liveHelp}}\n                            <li class=\"navUser-item\">\n                                <a class=\"navUser-action link-line\"\n                                   href=\"{{theme_settings.halo_header_liveHelpLink}}\"\n                                   aria-label=\"{{lang 'common.help'}}\"\n                                >\n                                    <span class=\"d-inline-block v-a-middle\">{{lang 'common.help'}}</span>\n                                    <svg class=\"icon d-inline-block v-a-middle\" role=\"img\" aria-label=\"{{lang 'common.help'}}\">\n                                        <title>{{lang 'common.help'}}</title>\n                                        <desc>{{lang 'common.help'}}</desc>\n                                        <use xlink:href=\"#icon-helper\"></use>\n                                    </svg>\n                                </a>\n                            </li>\n                        {{/if}}\n                        {{#or customer (unless settings.hide_price_from_guests)}}\n                            {{#if currency_selector.currencies.length '>' 1}}\n                                <li class=\"navUser-item navUser-item--currency left\">\n                                    <a class=\"navUser-action navUser-action--currencySelector link-line has-dropdown\"\n                                        href=\"#\"\n                                        data-dropdown=\"currencySelection\"\n                                        aria-controls=\"currencySelection\"\n                                        aria-expanded=\"false\"\n                                        aria-label=\"{{lang 'common.currency' code=currency_selector.active_currency_code}}\"\n                                    >\n                                        <div class=\"cur-flag d-inline-block v-a-middle\">{{{currency_selector.active_currency_flag}}}</div>\n                                        <span class=\"cur-code d-inline-block v-a-middle\">{{currency_selector.active_currency_code}}</span>\n                                        <svg class=\"icon d-inline-block v-a-middle\" role=\"img\" aria-label=\"{{currency_selector.active_currency_code}}\">\n                                            <title>{{currency_selector.active_currency_code}}</title>\n                                            <desc>{{currency_selector.active_currency_code}}</desc>\n                                            <use xlink:href=\"#icon-dropdown\"></use>\n                                        </svg>\n                                    </a>\n                                    <ul class=\"dropdown-menu\" id=\"currencySelection\" data-dropdown-content aria-hidden=\"true\" tabindex=\"-1\">\n                                        <li class=\"dropdown-menu-title\">\n                                            <span class=\"text\">{{lang 'common.select_currency'}}</span>\n                                        </li>\n                                        {{#each currency_selector.currencies}}\n                                            <li class=\"dropdown-menu-item link-line\">\n                                                <a href=\"{{{switch_url}}}\"\n                                                    data-currency-code=\"{{{code}}}\"\n                                                    {{#if cart_currency_switch_url.length '>' 0 }}\n                                                    data-cart-currency-switch-url=\"{{{cart_currency_switch_url}}}\"\n                                                    {{/if}}\n                                                    data-warning=\"{{lang 'common.currency_switch_promotion'}}\"\n                                                    {{#if is_active}} class=\"is-active\"{{/if}}\n                                                >\n                                                    {{{flag}}}\n                                                    <span class=\"text color-black\">{{name}}</span>\n                                                </a>\n                                            </li>\n                                        {{/each}}\n                                    </ul>\n                                </li>\n                            {{/if}}\n                        {{/or}}\n                    </ul>\n                </nav>                \n            </div>\n        </div>\n    </div>\n    {{{region name=\"header_navigation_bottom--global\"}}}\n</header>\n{{{region name=\"header_bottom--global\"}}}\n{{{region name=\"header_bottom\"}}}\n","components/common/navigation":"<nav class=\"navUser\">\n    <ul class=\"navUser-section navUser-section--action navUser-section--alt\">\n        <li class=\"navUser-item navUser-item--alt navUser-item--account center\">\n            {{#if customer}}\n                <a class=\"navUser-action navUser-action--account link-line\"\n                href=\"{{urls.account.index}}\"\n                aria-label=\"{{lang 'common.account'}}\"\n                data-login-form\n                >\n                    <svg class=\"icon\" role=\"img\" aria-label=\"{{lang 'common.account'}}\">\n                        <title>{{lang 'common.account'}}</title>\n                        <desc>{{lang 'common.account'}}</desc>\n                        <use xlink:href=\"#icon-account\"></use>\n                    </svg>\n                    <span class=\"item-label d-inline-block d-none-xxxl\">{{lang 'create_account.account_default'}} {{customer.name}}.</span>\n                </a>\n            {{else}}\n                <a class=\"navUser-action link-line\"\n                href=\"{{urls.auth.login}}\"\n                aria-label=\"{{lang 'common.login'}}\"\n                data-login-form\n                >\n                    <svg class=\"icon\" role=\"img\" aria-label=\"{{lang 'common.login'}}\">\n                        <title>{{lang 'common.login'}}</title>\n                        <desc>{{lang 'common.login'}}</desc>\n                        <use xlink:href=\"#icon-account\"></use>\n                    </svg>\n                    {{#if settings.account_creation_enabled}}\n                        <span class=\"item-label d-inline-block d-none-xxxl\">{{lang 'common.login'}}</span>\n                    {{else}}\n                        <span class=\"item-label d-inline-block d-none-xxxl\">{{lang 'common.login_2'}}</span>\n                    {{/if}}\n                </a>\n            {{/if}}\n        </li>\n        {{#if settings.show_wishlist}}\n            <li class=\"navUser-item navUser-item--alt navUser-item--wishlist center\">\n                <a class=\"navUser-action link-line\" href=\"{{urls.account.wishlists.all}}\">\n                    <div class=\"navUser-icon\">\n                        <svg class=\"icon\" role=\"img\" aria-label=\"{{lang 'account.wishlists.heading'}}\">\n                            <title>{{lang 'account.wishlists.heading'}}</title>\n                            <desc>{{lang 'account.wishlists.heading'}}</desc>\n                            <use xlink:href=\"#icon-heart\"></use>\n                        </svg>\n                        <span class=\"item-label d-inline-block d-none-xxxl\">{{lang 'common.wishlist'}}</span>\n                    </div>\n                </a>\n            </li>\n        {{/if}}\n        <li class=\"navUser-item navUser-item--alt navUser-item--cart center\">\n            <a class=\"navUser-action link-line b-r-100\" data-cart-preview data-options=\"align:right\" href=\"{{urls.cart}}\" aria-label=\"{{lang 'cart.nav_aria_label'}}\">\n                <div class=\"navUser-item-cartLabel\">\n                    <svg class=\"icon\" role=\"img\" aria-label=\"{{lang 'common.cart'}}\">\n                        <title>{{lang 'common.cart'}}</title>\n                        <desc>{{lang 'common.cart'}}</desc>\n                        <use xlink:href=\"#icon-cart\"></use>\n                    </svg>\n                    <span class=\"item-label d-inline-block d-none-xxxl\">{{lang 'common.cart'}}</span>\n                </div>\n                <div class=\"countPill d-inline-block v-a-middle center cart-quantity\">{{cart.quantity}}</div>\n            </a>\n        </li>\n    </ul>\n</nav>\n","components/common/navigation-dropdown":"<div class=\"navPage-subMenu navPage-subMenu-horizontal halo-fadeIn\" id=\"navPages-{{id}}\" tabindex=\"1\">\n    <div class=\"container container-1600\">\n        <ul class=\"navPage-subMenu-list\">\n            <li class=\"navPage-subMenu-item-parent navPage-subMenu-item-child navPage-subMenu-title d-none d-block-tb\">\n                <p class=\"navPage-subMenu-action navPages-action navPages-action-depth-max has-subMenu\">\n                    <span class=\"navPages-action-moreIcon\" aria-hidden=\"true\">\n                        <svg class=\"icon\"><use xlink:href=\"#icon-arrow-prev\" /></svg>\n                    </span>\n                    <a class=\"text\" href=\"{{url}}\"><span>{{name}}</span></a>\n                </p>\n            </li>\n            {{assignVar \"value\" name}}\n            {{assignVar \"urlValue\" url}}\n            {{#each children}}\n                <li class=\"navPage-subMenu-item-child{{#if children}} has-dropdown{{/if}}\">\n                    {{#if children}}\n                        {{#if @index '==' 0}}\n                            <a class=\"navPage-subMenu-action navPage-subMenu-action-all link-line navPages-action navPages-action-depth-max  d-none d-block-tb no-subMenu{{#if is_active}} activePage{{/if}}{{#if ../uppercase}} f-size-large f-bold{{/if}}\" href=\"{{getVar 'urlValue'}}\">\n                                <span>{{lang 'category.view_all.name_no_category'}} {{getVar 'value'}}</span>\n                            </a>\n                        {{/if}}\n                        <a class=\"navPage-subMenu-action link-line navPages-action navPages-action-depth-max has-subMenu{{#if is_active}} activePage{{/if}}{{#if ../uppercase}} f-size-large f-bold{{/if}}\" href=\"{{url}}\">\n                            <span>{{name}}</span><i class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\"><svg><use href=\"#icon-chevron-down\" /></svg></i>\n                        </a>\n                        <div class=\"navPage-subMenu navPage-subMenu-horizontal\" id=\"navPages-{{id}}\" tabindex=\"1\">\n                            <ul class=\"navPage-subMenu-list\">\n                                <li class=\"navPage-subMenu-item-parent navPage-subMenu-item-child navPage-subMenu-title d-none d-block-tb\">\n                                    <p class=\"navPage-subMenu-action navPages-action navPages-action-depth-max has-subMenu\">\n                                        <span class=\"navPages-action-moreIcon\" aria-hidden=\"true\">\n                                            <svg class=\"icon\"><use xlink:href=\"#icon-arrow-prev\" /></svg>\n                                        </span>\n                                        <a class=\"text\" href=\"{{url}}\"><span>{{name}}</span></a>\n                                    </p>\n                                </li>\n                                <li class=\"navPage-subMenu-item-child d-block capitalize\">\n                                    <a class=\"navPage-subMenu-action link-line navPages-action navPages-action-depth-max  d-none d-block-tbno-subMenu\" href=\"{{url}}\"><span>{{lang 'category.view_all.name' category=name}}</span></a>\n                                </li>\n                                {{#each children}}\n                                    <li class=\"navPage-subMenu-item-child capitalize\">\n                                        <a class=\"navPage-subMenu-action link-line navPages-action{{#if is_active}} activePage{{/if}} no-subMenu\" href=\"{{url}}\"><span>{{name}}</span></a>\n                                    </li>\n                                {{/each}}\n                            </ul>\n                        </div>                    \n                    {{else}}\n                        <a class=\"navPage-subMenu-action link-line navPages-action{{#if is_active}} activePage{{/if}}{{#if ../uppercase}} f-size-large f-bold{{/if}} no-subMenu\" href=\"{{url}}\"><span>{{name}}</span></a>\n                    {{/if}}\n                </li>\n            {{/each}}\n        </ul>\n    </div>\n</div>\n","components/common/navigation-list":"{{#if children}}\n    <a class=\"navPages-action navPages-action-depth-max link-line has-subMenu is-root{{#if is_active}} activePage{{/if}} uppercase\"\n       href=\"{{url}}\"\n       aria-label=\"{{name}}\"\n    >\n        <span>{{name}}</span>\n        <i class=\"icon navPages-action-moreIcon d-none d-inline-block-tb\" aria-hidden=\"true\">\n            <svg><use href=\"#icon-chevron-down\" /></svg>\n        </i>\n    </a>\n    {{> components/common/navigation-dropdown id=id uppercase=true}}\n{{else}}\n    <a class=\"navPages-action link-line{{#if is_active}} activePage{{/if}} no-subMenu uppercase\"\n       href=\"{{url}}\"\n       aria-label=\"{{name}}\"\n    >\n        <span>{{name}}</span>\n    </a>\n{{/if}}\n","components/common/navigation-menu":"<nav class=\"navPages\">\n    <ul class=\"navPages-list{{#if theme_settings.haloMegamenu}} navPages-list-megamenu{{/if}}\">\n        {{#if theme_settings.menu_home}}\n            <li class=\"navPages-item navPages-item-page\">\n                <a class=\"navPages-action link-line{{#if page_type '==' 'default'}} activePage{{/if}} uppercase\"\n                    href=\"/\"\n                    aria-label=\"Home\"\n                >\n                    <span>{{lang 'home.heading'}}</span>\n                </a>\n            </li>\n        {{/if}}\n        {{#each categories}}\n            <li class=\"navPages-item{{#if children}} has-dropdown{{/if}}\">\n                {{> components/common/navigation-list}}\n            </li>\n        {{/each}}\n        {{#unless theme_settings.hide_content_navigation}}\n            {{#each pages}}\n                <li class=\"navPages-item navPages-item-page{{#if children}} has-dropdown{{/if}}{{#if name '==' ../page.title}} activePage{{/if}} {{id}}\">\n                    {{> components/common/navigation-list id=(toLowerCase (dashcase name))}}\n                </li>\n            {{/each}}\n        {{/unless}}\n    </ul>\n</nav>\n","components/common/quick-search":"<form class=\"form\" onsubmit=\"return false\" data-url=\"{{urls.search}}\" data-quick-search-form>\n    <fieldset class=\"form-fieldset\">\n        <div class=\"form-field pos-relative\">\n            <label class=\"is-srOnly\" for=\"{{name}}\">{{lang \"search.quick_search.input_label\"}}</label>\n            <input class=\"form-input\" \n                   data-search-quick \n                   data-search=\"quickSearch\" \n                   name=\"{{name}}\" \n                   id=\"{{name}}\" \n                   data-error-message=\"{{lang 'search.error.empty_field'}}\" \n                   placeholder=\"{{lang 'search.quick_search.input_placeholder'}}\" \n                   autocomplete=\"off\">\n            <button type=\"submit\" class=\"button button--primary button--iconCustom pos-absolute\" aria-label=\"{{lang \"search.quick_search.input_label\"}}\"><svg class=\"icon\"><use xlink:href=\"#icon-search-2\"></use></svg></button>\n        </div>\n    </fieldset>\n</form>\n","components/common/store-logo":"<a href=\"{{urls.home}}\" class=\"header-logo__link\" data-header-logo-link>\n    {{#if settings.store_logo.image}}\n        {{#if theme_settings.logo_size '===' 'original'}}\n            <img class=\"header-logo-image-unknown-size\"\n                src=\"{{getImage settings.store_logo.image 'logo_size'}}\"\n                alt=\"{{settings.store_logo.title}}\"\n                title=\"{{settings.store_logo.title}}\"\n            >\n        {{else}}\n            <div class=\"header-logo-image-container\">\n                <img class=\"header-logo-image b-r-0\"\n                    src=\"{{getImage settings.store_logo.image 'logo_size'}}\"\n                    srcset=\"{{getImageSrcset1x2x settings.store_logo.image theme_settings.logo_size}}\"\n                    alt=\"{{settings.store_logo.title}}\"\n                    title=\"{{settings.store_logo.title}}\"\n                >\n            </div>\n        {{/if}}\n    {{else}}\n        <span class=\"header-logo-text\">{{settings.store_logo.title}}</span>\n    {{/if}}\n</a>\n"}
