{"components/common/navigation":"<nav class=\"navUser\">\n    <ul class=\"navUser-section navUser-section--action navUser-section--alt\">\n        <li class=\"navUser-item navUser-item--alt navUser-item--account center\">\n            {{#if customer}}\n                <a class=\"navUser-action navUser-action--account link-line\"\n                href=\"{{urls.account.index}}\"\n                aria-label=\"{{lang 'common.account'}}\"\n                data-login-form\n                >\n                    <svg class=\"icon\" role=\"img\" aria-label=\"{{lang 'common.account'}}\">\n                        <title>{{lang 'common.account'}}</title>\n                        <desc>{{lang 'common.account'}}</desc>\n                        <use xlink:href=\"#icon-account\"></use>\n                    </svg>\n                    <span class=\"item-label d-inline-block d-none-xxxl\">{{lang 'create_account.account_default'}} {{customer.name}}.</span>\n                </a>\n            {{else}}\n                <a class=\"navUser-action link-line\"\n                href=\"{{urls.auth.login}}\"\n                aria-label=\"{{lang 'common.login'}}\"\n                data-login-form\n                >\n                    <svg class=\"icon\" role=\"img\" aria-label=\"{{lang 'common.login'}}\">\n                        <title>{{lang 'common.login'}}</title>\n                        <desc>{{lang 'common.login'}}</desc>\n                        <use xlink:href=\"#icon-account\"></use>\n                    </svg>\n                    {{#if settings.account_creation_enabled}}\n                        <span class=\"item-label d-inline-block d-none-xxxl\">{{lang 'common.login'}}</span>\n                    {{else}}\n                        <span class=\"item-label d-inline-block d-none-xxxl\">{{lang 'common.login_2'}}</span>\n                    {{/if}}\n                </a>\n            {{/if}}\n        </li>\n        {{#if settings.show_wishlist}}\n            <li class=\"navUser-item navUser-item--alt navUser-item--wishlist center\">\n                <a class=\"navUser-action link-line\" href=\"{{urls.account.wishlists.all}}\">\n                    <div class=\"navUser-icon\">\n                        <svg class=\"icon\" role=\"img\" aria-label=\"{{lang 'account.wishlists.heading'}}\">\n                            <title>{{lang 'account.wishlists.heading'}}</title>\n                            <desc>{{lang 'account.wishlists.heading'}}</desc>\n                            <use xlink:href=\"#icon-heart\"></use>\n                        </svg>\n                        <span class=\"item-label d-inline-block d-none-xxxl\">{{lang 'common.wishlist'}}</span>\n                    </div>\n                </a>\n            </li>\n        {{/if}}\n        <li class=\"navUser-item navUser-item--alt navUser-item--cart center\">\n            <a class=\"navUser-action link-line b-r-100\" data-cart-preview data-options=\"align:right\" href=\"{{urls.cart}}\" aria-label=\"{{lang 'cart.nav_aria_label'}}\">\n                <div class=\"navUser-item-cartLabel\">\n                    <svg class=\"icon\" role=\"img\" aria-label=\"{{lang 'common.cart'}}\">\n                        <title>{{lang 'common.cart'}}</title>\n                        <desc>{{lang 'common.cart'}}</desc>\n                        <use xlink:href=\"#icon-cart\"></use>\n                    </svg>\n                    <span class=\"item-label d-inline-block d-none-xxxl\">{{lang 'common.cart'}}</span>\n                </div>\n                <div class=\"countPill d-inline-block v-a-middle center cart-quantity\">{{cart.quantity}}</div>\n            </a>\n        </li>\n    </ul>\n</nav>\n"}
