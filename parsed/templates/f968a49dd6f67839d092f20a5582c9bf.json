{"components/common/alert/alert-error":"<div class=\"alertBox alertBox--error\">\n    <div class=\"alertBox-column alertBox-icon\">\n        <icon glyph=\"ic-error\" class=\"icon\" aria-hidden=\"true\"><svg xmlns=\"http://www.w3.org/2000/svg\" width=\"24\" height=\"24\" viewBox=\"0 0 24 24\"><path d=\"M12 2C6.48 2 2 6.48 2 12s4.48 10 10 10 10-4.48 10-10S17.52 2 12 2zm1 15h-2v-2h2v2zm0-4h-2V7h2v6z\"></path></svg></icon>\n    </div>\n    <p class=\"alertBox-column alertBox-message\">\n        <span id=\"alertBox-message-text\">{{{this}}}</span>\n    </p>\n</div>\n","components/page/contact-us-form":"<form data-contact-form class=\"form bg-white m-0 p-40 p-30-tb p-20-10-mb b-r-24\" action=\"{{urls.contact_us_submit}}\" method=\"post\">\n    {{inject 'contactEmail' (lang 'forms.validate.contact.email_address')}}\n    {{inject 'contactQuestion' (lang 'forms.validate.contact.question')}}\n    {{#if forms.contact.error}}\n        <div id=\"contact-us-error\">{{> components/common/alert/alert-error forms.contact.error}}</div>\n    {{/if}}\n    <input type=\"hidden\" name=\"page_id\" id=\"page_id\" value=\"{{forms.contact.page_id}}\">\n    <div class=\"form-row\">\n        <p class=\"f-heading f-36 l-h-h m-b-40 color-heading f-bold\">Get in Touch</p>\n        {{#if forms.contact.name}}\n            <div class=\"form-field m-b m-b-18-tb m-b-12-mb\">\n                <label class=\"form-label d-none\" for=\"contact_fullname\">{{lang 'forms.contact_us.full_name'}}</label>\n                <input class=\"form-input h-100 f-size-large f-root-tb f-size-small-mb l-h-large l-h-extra-tb l-h-larger-mb p-20 p-10-20-tb b-r-40\" type=\"text\" placeholder=\"{{lang 'forms.contact_us.full_name'}}\" id=\"contact_fullname\" name=\"contact_fullname\" value=\"{{customer.name}}\">\n            </div>\n        {{/if}}\n\n        <div class=\"form-field m-b m-b-18-tb m-b-12-mb\">\n            <label class=\"form-label d-none\" for=\"contact_email\">{{lang 'forms.contact_us.email'}}<small>*</small></label>\n            <input class=\"form-input h-100 f-size-large f-root-tb f-size-small-mb l-h-large l-h-extra-tb l-h-larger-mb p-20 p-10-20-tb b-r-40\" type=\"text\" placeholder=\"{{lang 'forms.contact_us.email'}}\" id=\"contact_email\" name=\"contact_email\" value=\"{{customer.email}}\">\n        </div>\n\n        {{#if forms.contact.company}}\n            <div class=\"form-field m-b m-b-18-tb m-b-12-mb w-50 item-left\">\n                <label class=\"form-label d-none\" for=\"contact_companyname\">{{lang 'forms.contact_us.company'}}</label>\n                <input class=\"form-input h-100 f-size-large f-root-tb f-size-small-mb l-h-large l-h-extra-tb l-h-larger-mb p-20 p-10-20-tb b-r-40\" type=\"text\" placeholder=\"{{lang 'forms.contact_us.company'}}\" id=\"contact_companyname\" name=\"contact_companyname\">\n            </div>\n        {{/if}}\n\n        {{#if forms.contact.order}}\n            <div class=\"form-field m-b m-b-18-tb m-b-12-mb w-50  item-right\">\n                <label class=\"form-label d-none\" for=\"contact_orderno\">{{lang 'forms.contact_us.order'}}</label>\n                <input class=\"form-input h-100 f-size-large f-root-tb f-size-small-mb l-h-large l-h-extra-tb l-h-larger-mb p-20 p-10-20-tb b-r-40\" type=\"text\" placeholder=\"{{lang 'forms.contact_us.order'}}\" id=\"contact_orderno\" name=\"contact_orderno\">\n            </div>\n        {{/if}}\n\n        {{#if forms.contact.phone}}\n            <div class=\"form-field m-b m-b-18-tb m-b-12-mb\">\n                <label class=\"form-label d-none\" for=\"contact_phone\">{{lang 'forms.contact_us.phone'}}</label>\n                <input class=\"form-input h-100 f-size-large f-root-tb f-size-small-mb l-h-large l-h-extra-tb l-h-larger-mb p-20 p-10-20-tb b-r-40\" type=\"text\" placeholder=\"{{lang 'forms.contact_us.phone'}}\" id=\"contact_phone\" name=\"contact_phone\" value=\"{{customer.phone}}\">\n            </div>\n        {{/if}}\n\n        {{#if forms.contact.rma}}\n            <div class=\"form-field m-b m-b-18-tb m-b-12-mb\">\n                <label class=\"form-label d-none\" for=\"contact_rma\">{{lang 'forms.contact_us.rma'}}</label>\n                <input class=\"form-input h-100 f-size-large f-root-tb f-size-small-mb l-h-large l-h-extra-tb l-h-larger-mb p-20 p-10-20-tb b-r-40\" type=\"text\" placeholder=\"{{lang 'forms.contact_us.rma'}}\" id=\"contact_rma\" name=\"contact_rma\">\n            </div>\n        {{/if}}\n    </div>\n\n    <div class=\"form-field m-b m-b-18-tb m-b-12-mb\">\n        <label class=\"form-label d-none\" for=\"contact_question\">{{lang 'forms.contact_us.question'}}<small>*</small></label>\n        <textarea name=\"contact_question\" placeholder=\"{{lang 'forms.contact_us.question'}}\" id=\"contact_question\" rows=\"7\" cols=\"50\" class=\"form-input h-100 f-size-large f-root-tb f-size-small-mb l-h-large l-h-extra-tb l-h-larger-mb p-20 p-10-20-tb b-r-40\"></textarea>\n    </div>\n    {{#if forms.contact.manualcaptcha}}\n        <div class=\"form-field m-b m-b-18-tb m-b-12-mb\">\n            <label class=\"form-label d-none\" for=\"{{forms.contact.manualcaptcha.name}}\">{{lang 'forms.contact_us.manual_captcha_instruction'}}<small>*</small></label>\n            <div class=\"manual-captcha-input-container\">\n                <span class=\"question\">{{forms.contact.manualcaptcha.question}}</span>\n                <input class=\"form-input h-100 f-size-large f-root-tb f-size-small-mb l-h-large l-h-extra-tb l-h-larger-mb p-20 p-10-20-tb b-r-40\" type=\"text\" id=\"{{forms.contact.manualcaptcha.name}}\" name=\"{{forms.contact.manualcaptcha.name}}\" value=\"\" aria-disabled=\"true\" autocomplete=\"off\" required>\n            </div>\n        </div>\n    {{/if}}\n    {{{forms.contact.recaptcha.markup}}}\n    <div class=\"form-actions\">\n        <input class=\"button button--tertiary f-semibold uppercase f-size-large f-root-tb f-size-tiny-mb w-100 mi-w button--large m-0\" type=\"submit\" value=\"{{lang 'forms.contact_us.submit' }}\">\n    </div>\n</form>\n"}
