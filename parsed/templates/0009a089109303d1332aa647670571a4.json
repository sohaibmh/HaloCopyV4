wn-menu-item\">\n                <a class=\"btn-show-products{{#if sort '==' 'pricedesc'}} selected{{/if}} link-line\" data-index=\"8\" data-value=\"pricedesc\" aria-label=\"{{lang 'common.sorter.price_desc'}}\" href=\"?sort=pricedesc\">\n                    <span>{{lang 'common.sorter.price_desc'}}</span>\n                </a>\n            </li>\n            {{#if result_count}}\n                <li class=\"dropdown-menu-item\">\n                    <a class=\"btn-show-products{{#if sort '==' 'relevance'}} selected{{/if}} link-line\" data-index=\"9\" data-value=\"9\" aria-label=\"{{lang 'common.sorter.relevance'}}\" href=\"?sort=relevance\">\n                        <span>{{lang 'common.sorter.relevance'}}</span>\n                    </a>\n                </li>\n            {{/if}}\n        </ul>\n    </li>\n</ul>\n","components/products/filter":"<div class=\"actionBar-wrapper d-flex a-i-center j-c-between\">\n    <div class=\"actionBar-filter d-none-tb\">\n        <ul class=\"filter-dropdown b-r-24\">\n            <li class=\"filter-item pos-relative\">\n                <a class=\"filter-action d-block has-dropdown\"\n                    href=\"#\"\n                    data-dropdown=\"viewAs\"\n                    aria-controls=\"viewAs\"\n                    aria-expanded=\"false\"\n                >\n                    <span class=\"f-semibold d-none-xxl\">{{lang 'categories.layout'}}</span>\n                    <span class=\"dropdown-label\">{{#with theme_settings}}{{#if product_list_display_mode '==' 'list'}}{{lang 'categories.list_view'}}{{else if product_list_display_mode '==' 'grid2'}}{{lang 'categories.grid_view2'}}{{else if product_list_display_mode '==' 'grid3'}}{{lang 'categories.grid_view3'}}{{/if}}{{/with}}</span>\n                    <svg class=\"icon\" aria-hidden=\"true\"><use href=\"#icon-chevron-down\" /></svg>\n                </a>\n                <ul id=\"viewAs\" class=\"dropdown-menu b-r-24\" data-dropdown-content aria-hidden=\"true\" tabindex=\"-1\">\n                    <li class=\"dropdown-menu-item\">\n                        <a class=\"btn-view grid-view{{#if theme_settings.product_list_display_mode '==' 'list'}} selected{{/if}} link-line\" id=\"grid-view\" data-index=\"1\" aria-label=\"{{lang 'categories.list_view'}}\" href=\"#\">\n                            <span>{{lang 'categories.list_view'}}</span>\n                        </a>\n                    </li>\n                    <li class=\"dropdown-menu-item\">\n                        <a class=\"btn-view grid-view{{#if theme_settings.product_list_display_mode '==' 'grid2'}} selected{{/if}} link-line\" id=\"grid-view2\" data-index=\"2\" aria-label=\"{{lang 'categories.grid_view2'}}\" href=\"#\">\n                            <span>{{lang 'categories.grid_view2'}}</span>\n                        </a>\n                    </li> \n                    <li class=\"dropdown-menu-item\">\n                        <a class=\"btn-view grid-view{{#if theme_settings.product_list_display_mode '==' 'grid3'}} selected{{/if}} link-line\" id=\"grid-view3\" data-index=\"3\" aria-label=\"{{lang 'categories.grid_view3'}}\" href=\"#\">\n                            <span>{{lang 'categories.grid_view3'}}</span>\n                        </a>\n                    </li>\n                </ul>\n            </li>\n        </ul>\n    </div>\n    <div class=\"actionBar-filter\">\n        <form class=\"actionBar d-none-tb\" method=\"get\" data-sort-by=\"product\" data-show-products>\n            <ul class=\"filter-dropdown actionBar-dropdown b-r-24\">\n                <li class=\"filter-item pos-relative\">\n                    <a class=\"filter-action d-block has-dropdown\"\n                        href=\"#\"\n                        data-dropdown=\"limit\"\n                        aria-controls=\"limit\"\n                        aria-expanded=\"false\"\n                    >\n                        <span class=\"f-semibold d-none-xxl\">{{lang 'categories.product_per_page'